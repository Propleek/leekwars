include('_NEW_WORKINPROGRESS');

updateEnnemiesWithMessage();
//var maxOpe = 8000000 / getAliveEnemiesCount();
for(var enemy in getAliveEnemies()){
	debug('Enemi NÂ°'+enemy);
	getEnemyDamageMap(enemy, isSummon(enemy) ? 1000000 : 6000000);
}

maps_danger = getFinalDamageMapByLeek();
var max=0;
for(var v in maps_danger) if(v > max) max = v;
for(var cell: var score in maps_danger){
	if(max==0) mark(cell, COLOR_GREEN);
	else mark(cell, getColor(score/max*255, 255-(score/max*255), 0));
}
